<?xml version="1.0" encoding="utf-8"?>
<Patch>
  <Operation Class="PatchOperationConditional">
    <xpath>/Defs/ThingDef[defName="DeinotheriumTusk"]</xpath>
    <success>Always</success>
    <match Class="PatchOperationAdd">
      <xpath>/Defs</xpath>
      <value>
        <ThingDef ParentName="IvoryEngraved">
          <defName>engraved_DeinotheriumTusk</defName>
          <label>engraved deinotherium tusk</label>
          <description>An engraved deinotherium's tusk.</description>
          <graphicData>
            <texPath>Things/Item/Special/DeinotheriumTuskStatue</texPath>
            <graphicClass>Graphic_Single</graphicClass>
          </graphicData>
          <statBases>
            <MarketValue>1500</MarketValue>
            <MaxHitPoints>130</MaxHitPoints>
            <Mass>3.5</Mass>
            <Flammability>1</Flammability>
            <Beauty>25</Beauty>
          </statBases>
        </ThingDef>
        <RecipeDef ParentName="IvoryEngraving">
          <defName>engrave_DeinotheriumTusk</defName>
          <label>engrave deinotherium tusk</label>
          <description>Engrave deinotherium tusk with art.</description>
          <jobString>engraving deinotherium tusk</jobString>
          <ingredients>
            <li>
              <filter>
                <thingDefs>
                  <li>DeinotheriumTusk</li>
                </thingDefs>
              </filter>
              <count>1</count>
            </li>
          </ingredients>
          <products>
            <engraved_DeinotheriumTusk>1</engraved_DeinotheriumTusk>
          </products>
          <fixedIngredientFilter>
            <thingDefs>
              <li>DeinotheriumTusk</li>
            </thingDefs>
          </fixedIngredientFilter>
        </RecipeDef>
      </value>
    </match>
  </Operation>
