  <Operation Class="PatchOperationConditional">
    <xpath>/Defs/ThingDef[defName="BisonHorn_Westerado"]</xpath>
    <success>Always</success>
    <match Class="PatchOperationAdd">
      <xpath>/Defs</xpath>
      <value>
        <ThingDef ParentName="IvoryEngraved">
          <defName>engraved_BisonHorn_Westerado</defName>
          <label>engraved bison horn</label>
          <description>An engraved bison's horn.</description>
          <graphicData>
            <texPath>Things/Item/Special/BisonHornStatue</texPath>
            <graphicClass>Graphic_Single</graphicClass>
          </graphicData>
          <statBases>
            <MarketValue>150</MarketValue>
            <MaxHitPoints>100</MaxHitPoints>
            <Mass>0.3</Mass>
            <Flammability>0.9</Flammability>
            <Beauty>10</Beauty>
          </statBases>
        </ThingDef>
        <RecipeDef ParentName="IvoryEngraving">
          <defName>engrave_BisonHorn_Westerado</defName>
          <label>engrave bison horn</label>
          <description>Engrave bison horn with art.</description>
          <jobString>engraving bison horn</jobString>
          <ingredients>
            <li>
              <filter>
                <thingDefs>
                  <li>BisonHorn_Westerado</li>
                </thingDefs>
              </filter>
              <count>1</count>
            </li>
          </ingredients>
          <products>
            <engraved_BisonHorn_Westerado>1</engraved_BisonHorn_Westerado>
          </products>
          <fixedIngredientFilter>
            <thingDefs>
              <li>BisonHorn_Westerado</li>
            </thingDefs>
          </fixedIngredientFilter>
        </RecipeDef>
      </value>
    </match>
  </Operation>
