      <li>BuildingsArt</li>
    </thingCategories>
    <building>
      <ai_chillDestination>false</ai_chillDestination>
      <isInert>true</isInert>
      <isEdifice>false</isEdifice>
      <canPlaceOverWall>false</canPlaceOverWall>
    </building>
    <statBases>
      <Flammability>1.0</Flammability>
      <SellPriceFactor>1.10</SellPriceFactor>
      <MeditationFocusStrength>0.0</MeditationFocusStrength>
    </statBases>
    <comps>
      <li>
        <compClass>CompQuality</compClass>
      </li>
      <li Class="CompProperties_Art">
        <nameMaker>NamerArtSculpture</nameMaker>
        <descriptionMaker>ArtDescription_Sculpture</descriptionMaker>
        <canBeEnjoyedAsArt>true</canBeEnjoyedAsArt>
      </li>
      <li Class="CompProperties_MeditationFocus">
        <statDef>MeditationFocusStrength</statDef>
        <focusTypes>
          <li>Artistic</li>
        </focusTypes>
        <offsets>
          <li Class="FocusStrengthOffset_Quality">
            <curve>
              <points>
                <li>(0,0.12)</li>
                <!-- awful -->
                <li>(1,0.16)</li>
                <!-- poor -->
                <li>(2,0.20)</li>
                <!-- normal -->
                <li>(3,0.22)</li>
                <!-- good -->
                <li>(4,0.24)</li>
                <!-- excellent -->
                <li>(5,0.26)</li>
                <!-- masterwork -->
                <li>(6,0.28)</li>
                <!-- legendary -->
              </points>
            </curve>
          </li>
          <li Class="FocusStrengthOffset_BuildingDefsWithQuality">
            <defs>
              <li>SculptureSmall</li>
              <li>SculptureLarge</li>
              <li>SculptureGrand</li>
            </defs>
            <radius>9.9</radius>
            <maxBuildings>8</maxBuildings>
            <focusPerQuality>
              <points>
                <li>(0,0.0)</li>
                <!-- awful -->
                <li>(1,0.0)</li>
                <!-- poor -->
                <li>(2,0.01)</li>
                <!-- normal -->
                <li>(3,0.01)</li>
                <!-- good -->
                <li>(4,0.01)</li>
                <!-- excellent -->
                <li>(5,0.02)</li>
                <!-- masterwork -->
                <li>(6,0.02)</li>
                <!-- legendary -->
              </points>
            </focusPerQuality>
            <explanationKey>MeditationFocusPerSculpture</explanationKey>
            <explanationKeyAbstract>MeditationFocusPerSculptureAbstract</explanationKeyAbstract>
          </li>
        </offsets>
      </li>
    </comps>
    <placeWorkers>
      <li>PlaceWorker_MeditationOffsetBuildingsNear</li>
    </placeWorkers>
    <inspectorTabs>
      <li>ITab_Art</li>
    </inspectorTabs>
    <tradeTags>
      <li>Art</li>
    </tradeTags>
    <resourceReadoutPriority>Middle</resourceReadoutPriority>
    <stackLimit>1</stackLimit>
    <clearBuildingArea>false</clearBuildingArea>
    <thingSetMakerTags>
      <li>AnimalPart</li>
      <li>RewardStandardLowFreq</li>
    </thingSetMakerTags>
  </ThingDef>
