<?xml version="1.0" encoding="utf-8"?>
<Patch>
  <Operation Class="PatchOperationConditional">
    <xpath>/Defs/ThingDef[defName="DeerAntler_Westerado"]</xpath>
    <success>Always</success>
    <match Class="PatchOperationConditional">
      <xpath>/Defs/ThingDef[defName="AYTallow"]</xpath>
      <success>Always</success>
      <match Class="PatchOperationAdd">
        <xpath>/Defs</xpath>
        <value>
          <ThingDef ParentName="IvoryEngraved">
            <defName>DeerAntlerChandelier</defName>
            <label>deer antler chandelier</label>
            <description>A large chandelier made from the antlers of deer.</description>
            <graphicData>
              <texPath>Things/Item/Special/AntlerChandelier</texPath>
              <graphicClass>Graphic_Single</graphicClass>
              <drawSize>(3,3)</drawSize>
            </graphicData>
            <size>(2,2)</size>
            <tickerType>Normal</tickerType>
            <statBases>
              <MarketValue>1200</MarketValue>
              <MaxHitPoints>200</MaxHitPoints>
              <Mass>8</Mass>
              <Flammability>0.15</Flammability>
              <Beauty>40</Beauty>
            </statBases>
            <comps>
              <li Class="CompProperties_Refuelable">
                <fuelConsumptionRate>1</fuelConsumptionRate>
                <fuelCapacity>60.0</fuelCapacity>
                <fuelConsumptionPerTickInRain>0.0006</fuelConsumptionPerTickInRain>
                <fuelFilter>
                  <thingDefs>
                    <li>AYTallow</li>
                  </thingDefs>
                </fuelFilter>
                <destroyOnNoFuel>false</destroyOnNoFuel>
                <initialFuelPercent>100</initialFuelPercent>
              </li>
              <li Class="CompProperties_Flickable" />
              <li Class="CompProperties_Glower">
                <glowRadius>10</glowRadius>
                <glowColor>(252,187,113,0)</glowColor>
              </li>
              <li Class="CompProperties_HeatPusher">
                <heatPerSecond>0.5</heatPerSecond>
                <heatPushMaxTemperature>23</heatPushMaxTemperature>
              </li>
              <li Class="CustomLights.CompProperties_CustomLights">
                <lightSize>0.05</lightSize>
                <lightOffsets>
                  <li>(0.73,0.1,-0.18)</li>
                  <li>(-0.71,0.1,-0.18)</li>
                  <li>(0.43,0.1,-0.17)</li>
                  <li>(-0.41,0.1,-0.17)</li>
                  <li>(0.33,0.1,0.35)</li>
                  <li>(-0.31,0.1,0.35)</li>
                </lightOffsets>
              </li>
            </comps>
            <placeWorkers>
              <li>CustomLights.PlaceWorker_IsUnderRoof</li>
            </placeWorkers>
          </ThingDef>
          <RecipeDef ParentName="IvoryEngraving">
            <defName>make_DeerAntlerChandelier</defName>
            <label>make deer antler chandelier</label>
            <description>Make a deer antler chandelier.</description>
            <jobString>making deer antler chandelier</jobString>
            <ingredients Inherit="False">
              <li>
                <filter>
                  <thingDefs>
                    <li>DeerAntler_Westerado</li>
                  </thingDefs>
                </filter>
                <count>10</count>
              </li>
              <li>
                <filter>
                  <thingDefs>
                    <li>AYTallow</li>
                  </thingDefs>
                </filter>
                <count>120</count>
              </li>
              <li>
                <filter>
                  <thingDefs>
                    <li>Cloth</li>
                  </thingDefs>
                </filter>
                <count>12</count>
              </li>
            </ingredients>
            <products>
              <DeerAntlerChandelier>1</DeerAntlerChandelier>
            </products>
            <fixedIngredientFilter>
              <thingDefs>
                <li>DeerAntler_Westerado</li>
                <li>AYTallow</li>
                <li>Cloth</li>
              </thingDefs>
            </fixedIngredientFilter>
          </RecipeDef>
        </value>
      </match>
    </match>
  </Operation>
