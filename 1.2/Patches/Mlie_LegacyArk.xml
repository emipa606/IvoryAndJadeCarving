<?xml version="1.0" encoding="utf-8"?>
<Patch>
  <Operation Class="PatchOperationConditional">
    <xpath>/Defs/ThingDef[defName="LonghornHorn"]</xpath>
    <success>Always</success>
    <match Class="PatchOperationAdd">
      <xpath>/Defs</xpath>
      <value>
        <ThingDef ParentName="IvoryEngraved">
          <defName>engraved_LonghornHorn</defName>
          <label>engraved longhorn horn</label>
          <description>An engraved longhorn's horn.</description>
          <graphicData>
            <texPath>Things/Item/Special/LonghornHornStatue</texPath>
            <graphicClass>Graphic_Single</graphicClass>
          </graphicData>
          <statBases>
            <MarketValue>150</MarketValue>
            <MaxHitPoints>100</MaxHitPoints>
            <Mass>1.3</Mass>
            <Flammability>0.9</Flammability>
            <Beauty>12</Beauty>
          </statBases>
        </ThingDef>
        <RecipeDef ParentName="IvoryEngraving">
          <defName>engrave_LonghornHorn</defName>
          <label>engrave longhorn horn</label>
          <description>Engrave longhorn horn with art.</description>
          <jobString>engraving longhorn horn</jobString>
          <ingredients>
            <li>
              <filter>
                <thingDefs>
                  <li>LonghornHorn</li>
                </thingDefs>
              </filter>
              <count>1</count>
            </li>
          </ingredients>
          <products>
            <engraved_LonghornHorn>1</engraved_LonghornHorn>
          </products>
          <fixedIngredientFilter>
            <thingDefs>
              <li>LonghornHorn</li>
            </thingDefs>
          </fixedIngredientFilter>
        </RecipeDef>
      </value>
    </match>
  </Operation>
