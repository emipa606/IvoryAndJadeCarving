<Patch>
  <Operation Class="PatchOperationConditional">
    <xpath>/Defs/ThingDef[defName="DankPyon_DireBoarTusk"]</xpath>
    <success>Always</success>
    <match Class="PatchOperationAdd">
      <xpath>/Defs</xpath>
      <value>
        <ThingDef ParentName="IvoryEngraved">
          <defName>engraved_DankPyon_DireBoarTusk</defName>
          <label>engraved dire boar tusk</label>
          <description>An engraved dire boar tusk.</description>
          <graphicData>
            <texPath>Things/Item/Special/DankPyon_DireBoarTuskStatue</texPath>
            <graphicClass>Graphic_Single</graphicClass>
          </graphicData>
          <statBases>
            <MarketValue>120</MarketValue>
            <MaxHitPoints>130</MaxHitPoints>
            <Mass>2</Mass>
            <Flammability>0.9</Flammability>
            <Beauty>15</Beauty>
          </statBases>
        </ThingDef>
        <RecipeDef ParentName="IvoryEngraving">
          <defName>engrave_DankPyon_DireBoarTusk</defName>
          <label>engrave dire boar tusk</label>
          <description>Engrave dire boar tusk with art.</description>
          <jobString>engraving dire boar tusk</jobString>
          <ingredients>
            <li>
              <filter>
                <thingDefs>
                  <li>DankPyon_DireBoarTusk</li>
                </thingDefs>
              </filter>
              <count>1</count>
            </li>
          </ingredients>
          <products>
            <engraved_DankPyon_DireBoarTusk>1</engraved_DankPyon_DireBoarTusk>
          </products>
          <fixedIngredientFilter>
            <thingDefs>
              <li>DankPyon_DireBoarTusk</li>
            </thingDefs>
          </fixedIngredientFilter>
        </RecipeDef>
      </value>
    </match>
  </Operation>
