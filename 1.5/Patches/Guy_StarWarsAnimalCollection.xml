<?xml version="1.0" encoding="utf-8"?>
<Patch>
  <Operation Class="PatchOperationConditional">
    <xpath>/Defs/ThingDef[defName="BanthaHorn"]</xpath>
    <success>Always</success>
    <match Class="PatchOperationAdd">
      <xpath>/Defs</xpath>
      <value>
        <ThingDef ParentName="IvoryEngraved">
          <defName>engraved_BanthaHorn</defName>
          <label>engraved bantha horn</label>
          <description>An engraved bantha's horn.</description>
          <graphicData>
            <texPath>Things/Item/Special/BanthaHornStatue</texPath>
            <graphicClass>Graphic_Single</graphicClass>
          </graphicData>
          <statBases>
            <MarketValue>250</MarketValue>
            <MaxHitPoints>90</MaxHitPoints>
            <Mass>3.5</Mass>
            <Flammability>0.4</Flammability>
            <Beauty>19</Beauty>
          </statBases>
        </ThingDef>
        <RecipeDef ParentName="IvoryEngraving">
          <defName>engrave_BanthaHorn</defName>
          <label>engrave bantha horn</label>
          <description>Engrave bantha horn with art.</description>
          <jobString>engraving bantha horn</jobString>
          <ingredients>
            <li>
              <filter>
                <thingDefs>
                  <li>BanthaHorn</li>
                </thingDefs>
              </filter>
              <count>1</count>
            </li>
          </ingredients>
          <products>
            <engraved_BanthaHorn>1</engraved_BanthaHorn>
          </products>
          <fixedIngredientFilter>
            <thingDefs>
              <li>BanthaHorn</li>
            </thingDefs>
          </fixedIngredientFilter>
        </RecipeDef>
      </value>
    </match>
  </Operation>
